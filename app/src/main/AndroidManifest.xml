<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.formularioapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" android:maxSdkVersion="32" />



    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FormularioApp"
        tools:targetApi="31">

        <activity
            android:name=".Registrarse_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".FeedActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Login_Activity"
            android:exported="false" />

        <activity
            android:name=".MiCuentaActivity"
            android:exported="false"
            android:label="Mi Cuenta" />

        <activity
            android:name=".HistorialdeCompras_Activity"
            android:exported="false"
            android:label="Historial de Compras" />

        <activity
            android:name=".Micarrito_Activity"
            android:exported="false"
            android:label="Mi Carrito" />

        <activity
            android:name=".Metododepago_Activity"
            android:exported="false"
            android:label="Métodos de Pago" />

        <activity
            android:name=".PublicarArticulo_Activity"
            android:exported="false"
            android:label="Mis Artículos en Venta"
            android:parentActivityName=".FeedActivity" />

        <activity
            android:name=".FacturaActivity"
            android:exported="false"
            android:label="Factura" />

        <activity
            android:name=".Cuenta_acceso"
            android:exported="false"
            android:label="Acceso a Cuenta" />


    </application>
</manifest>